-- Call integrated unit tests in the XQuery module

   xq = mod.load('xquery')

-----------------------------------------------------------------------------------------------------------------------

@Test function UnitTests()
   local passed, total = mod.test(xq)
   assert(passed is total, "Passed " .. passed .. " of " .. total .. " XQuery unit tests.  Use --log-api for details.")
   print('Passed ' .. total .. ' embedded unit tests.')
end
