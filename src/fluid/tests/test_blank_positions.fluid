-- Test blank in different positions

function testBlankFirst()
   print("Testing _, x = 10, 20")
   local _, x = 10, 20
   print("x = " .. x)
end

function testBlankSecond()
   print("Testing x, _ = 10, 20")
   local x, _ = 10, 20
   print("x = " .. x)
end

function testBlankMiddle()
   print("Testing x, _, y = 10, 20, 30")
   local x, _, y = 10, 20, 30
   print("x = " .. x)
   print("y = " .. y)
end

testBlankFirst()
testBlankSecond()
testBlankMiddle()
