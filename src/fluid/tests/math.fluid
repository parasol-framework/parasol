
function testRound()
   -- Test basic rounding (no decimal places)
   assert(math.round(3.7) == 4, 'math.round(3.7) should be 4')
   assert(math.round(3.2) == 3, 'math.round(3.2) should be 3')
   assert(math.round(3.5) == 4, 'math.round(3.5) should be 4')
   assert(math.round(-3.7) == -4, 'math.round(-3.7) should be -4')
   assert(math.round(-3.2) == -3, 'math.round(-3.2) should be -3')
   assert(math.round(-3.5) == -3, 'math.round(-3.5) should be -3')

   -- Test with decimal places
   assert(math.round(3.14159, 2) == 3.14, 'math.round(3.14159, 2) should be 3.14')
   assert(math.round(3.14159, 3) == 3.142, 'math.round(3.14159, 3) should be 3.142')
   assert(math.round(123.456, 1) == 123.5, 'math.round(123.456, 1) should be 123.5')
   assert(math.round(123.456, 0) == 123, 'math.round(123.456, 0) should be 123')

   -- Test edge cases
   assert(math.round(0) == 0, 'math.round(0) should be 0')
   assert(math.round(0, 2) == 0, 'math.round(0, 2) should be 0')

   -- Test negative decimal places (rounding to tens, hundreds, etc.)
   assert(math.round(1234.5, -1) == 1230, 'math.round(1234.5, -1) should be 1230')
   assert(math.round(1234.5, -2) == 1200, 'math.round(1234.5, -2) should be 1200')

   print('All math.round() tests passed!')
end

-----------------------------------------------------------------------------------------------------------------------

   return {
      tests = {
         'testRound'
      }
   }
