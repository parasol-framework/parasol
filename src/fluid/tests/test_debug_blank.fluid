-- Debug test to understand register allocation

function debug1()
   print("Test 1: local x = 10")
   local x = 10
   print("x = " .. x)
end

function debug2()
   print("Test 2: local x, y = 10, 20")
   local x, y = 10, 20
   print("x = " .. x)
   print("y = " .. y)
end

function debug3()
   print("Test 3: local x, _ = 10, 20")
   local x, _ = 10, 20
   print("x = " .. x)
end

debug1()
debug2()
debug3()
